

<%- if controller_name != 'sessions' %>

    <%= link_to "Ya tengo cuenta", new_session_path(resource_name) , class:"button small  warning-inverted  medium-6 small-12" %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
    <%= link_to "Registrar", new_registration_path(resource_name) , class:"button small  inverted medium-6 small-12" %>

<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
    <%= link_to "Olvide contr.", new_password_path(resource_name) , class:"button small  warning-inverted  medium-5 small-12" %><br />

<% end %>
<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
    <%= link_to "No recibi confirmacion", new_confirmation_path(resource_name) , class:"button small  warning-inverted medium-5 small-12" %><br />

<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
    <%= link_to "No recibi desbloqueo", new_unlock_path(resource_name) , class:"button small  warning-inverted" %><br />

<% end %>

<%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
        <%= link_to "Ingresar como  #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider) , class:"button small  secondary" %><br />
   
  <% end %>
<% end %>
